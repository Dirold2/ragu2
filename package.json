{
  "name": "ragu2",
  "version": "0.0.8.2",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "rm -rf build && mkdir build && tsc",
    "dev": "node --enable-source-maps --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm/transpile-only src/dev.ts",
    "start": "node --enable-source-maps --experimental-specifier-resolution=node build/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"No tests specified\" && exit 0",
    "prepare": "husky",
    "pm2:start": "pm2 start build/main.js --name ragu2",
    "pm2:restart": "pm2 restart ragu2",
    "pm2:stop": "pm2 stop ragu2",
    "pm2:delete": "pm2 delete ragu2",
    "pm2:list": "pm2 list"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.14.4",
    "@prisma/client": "^5.19.1",
    "@types/async-retry": "^1.4.8",
    "@types/sodium-native": "^2.3.9",
    "async-retry": "^1.3.3",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "discord-player": "^6.7.1",
    "dotenv": "^16.4.5",
    "node-cache": "^5.1.2",
    "play-dl": "^1.9.7",
    "prisma": "^5.19.1",
    "prom-client": "^15.1.3",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "ym-api-meowed": "^1.3.4",
    "ytmusic-api": "^5.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@discordjs/voice": "^0.17.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/utilities": "^7.0.3",
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.5",
    "chokidar": "^4.0.0",
    "discord.js": "^14.16.2",
    "discordx": "^11.12.1",
    "eslint": "^9.10.0",
    "husky": "^9.1.6",
    "opusscript": "^0.0.8",
    "prettier": "^3.3.3",
    "sodium-native": "^4.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": ">=4.7.4 <5.6.0",
    "typescript-eslint": "^8.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  }
}
